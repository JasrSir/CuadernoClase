<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_creat_modif_estud"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/activity_horizontal_margin"
    android:orientation="vertical"
    tools:context="jasrsir.com.cuadernoclasejasr.CreatModifEstud_Activity">

    <TextView
        android:id="@+id/widget33"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"
        android:text="Nombre Esudiante" />
    <EditText
        android:id="@+id/edtnombreEst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:inputType="textPersonName"
        android:textSize="18sp" />
    <TextView
        android:id="@+id/widget35"
        android:layout_width="match_parent"
        android:inputType="textPersonName"
        android:layout_height="wrap_content"
        android:text="Apellidos Estudiante" />
    <EditText
        android:id="@+id/edtapellidEst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:text=""
        android:textSize="16sp" />
    <TextView
        android:id="@+id/widget38"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DirecciÃ³n" />
    <EditText
        android:id="@+id/edtdireccionEst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:text=""
        android:textSize="16sp" />
    <TextView
        android:id="@+id/widget380"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Provincia" />
    <EditText
        android:id="@+id/edtprovinciaEst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:text=""
        android:textSize="16sp" />
    <TextView
        android:id="@+id/widget3f7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Codigo Postal:" />
    <EditText
        android:id="@+id/edtCodPosEst"
        android:layout_width="match_parent"
        android:inputType="number"
        android:layout_height="wrap_content"
        android:maxLength="5"
        android:text=""
        android:textSize="16sp" />
    <TextView
        android:id="@+id/widget37"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Email:" />
    <EditText
        android:id="@+id/edtEmailEst"
        android:layout_width="match_parent"
        android:inputType="textEmailAddress"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="16sp" />
    <TextView
        android:id="@+id/widget374"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Telefono:" />
    <EditText
        android:id="@+id/edtTelEst"
        android:layout_width="match_parent"
        android:inputType="phone"
        android:layout_height="wrap_content"
        android:text=""
        android:maxLength="9"
        android:textSize="16sp" />
    <LinearLayout
        android:id="@+id/widget39"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_gravity="center_horizontal">
        <Button
            android:id="@+id/accept"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Accept" />
        <Button
            android:id="@+id/cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cancel" />
    </LinearLayout>
</LinearLayout>
